{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "scripts": {
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org",
            "pre-install-cmd": [
                "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
            ],
            "pre-update-cmd": [
                "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
            ],
            "post-install-cmd": [
                "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
            ],
            "post-update-cmd": [
                "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
                "rm -f web/robots.txt"
            ]

        }
    ],
    "require": {
        "bower-asset/leaflet": "1.0.3",
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.1",
        "drupal/antibot": "^2.0",
        "drupal/block_visibility_groups": "^1.4",
        "drupal/bootstrap": "^3.24",
        "drupal/contact_emails": "^1.20",
        "drupal/contact_storage": "^1.1",
        "drupal/contact_tools": "^1.1",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/ctools": "^3.7",
        "drupal/devel": "^4.1",
        "drupal/ds": "^3.13",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/field_group": "^3.2",
        "drupal/field_image_style": "^1.1",
        "drupal/imagemagick": "^3.3",
        "drupal/imce": "^2.4",
        "drupal/markdown": "^3.0@RC",
        "drupal/media_remote": "^1.3",
        "drupal/metatag": "^1.19",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.6",
        "drupal/paragraphs": "^1.14",
        "drupal/pathauto": "^1.10",
        "drupal/photoswipe": "^3.1",
        "drupal/redirect": "^1.7",
        "drupal/robotstxt": "^1.4",
        "drupal/scheduler": "^1.4",
        "drupal/snippet_manager": "^1.8",
        "drupal/swiftmailer": "^2.2",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_tweak": "^3.1",
        "drupal/views_templates": "^1.1",
        "drupal/youtube": "^1.2",
        "drush/drush": "^11.0",
        "erusev/parsedown": "^1.7",
        "michelf/php-markdown": "^1.9",
        "npm-asset/chosen-js": "^1.8",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "third-party/required-plugin": true,
            "my-organization/*": true,
            "unnecessary/plugin": false,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "installer-paths": {
            "web/libraries/chosen": ["npm-asset/chosen-js"],
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library", "type:bower-asset", "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches" : {
            "drupal/photoswipe": {
                "Support Media and Entity References": "https://www.drupal.org/files/issues/photoswipe-caption-unset-2666636-3-D8.patch"
            },
            "drupal/tvi": {
                "Route enhancer": "https://www.drupal.org/files/issues/2018-08-17/route_enhancer-2993234-3.patch"
            }
        }
    }
}
